{
    "private": true,
    "version": "1.0.0",
    "scripts": {
        "dev": "npm run development",
        "development": "mix --config=webpack.bootstrap.mix.js",
        "watch": "mix watch --config=webpack.bootstrap.mix.js",
        "watch-poll": "mix watch --config=webpack.bootstrap.mix.js -- --watch-options-poll=1000",
        "hot": "mix watch --hot --config=webpack.bootstrap.mix.js",
        "prod": "npm run production",
        "production": "mix --production --config=webpack.bootstrap.mix.js",
        "clean": "rimraf public/assets && npm run prod"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.5.1",
        "@popperjs/core": "2.11.8",
        "axios": "^1.6.5",
        "bootstrap": "5.3.2",
        "bootstrap-icons": "^1.11.3",
        "bootstrap-maxlength": "^1.10.1",
        "bootstrap-daterangepicker": "^3.1.0",
        "chart.js": "^4.4.1",
        "datatables.net": "^1.13.8",
        "datatables.net-bs5": "^1.13.8",
        "datatables.net-buttons": "^2.4.2",
        "datatables.net-buttons-bs5": "^2.4.2",
        "datatables.net-responsive": "^2.5.0",
        "datatables.net-responsive-bs5": "^2.5.0",
        "dropzone": "^5.9.3",
        "flatpickr": "^4.6.13",
        "inputmask": "^5.0.8",
        "jquery": "3.7.1",
        "line-awesome": "^1.3.0",
        "moment": "^2.30.1",
        "nouislider": "^15.7.1",
        "apexcharts": "3.45.1",
        "select2": "^4.1.0-rc.0",
        "sweetalert2": "11.4.8",
