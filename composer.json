{
    "name": "keenthemes/metronic-laravel",
    "type": "project",
    "description": "The Metronic in the Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "anlutro/l4-settings": "^1.0",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "jackiedo/log-reader": "^2.3",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^6.0",
        "laravel/tinker": "^3.0",
        "livewire/livewire": "^3.3.2",
        "psr/simple-cache": "^2.0",
        "rappasoft/laravel-livewire-tables": "^3.3.2",
        "ratchet/pawl": "^0.4.1",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.0",
        "yajra/laravel-datatables": "^10.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.0",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^2.0",
        "laravel/breeze": "^1.14",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php",
            "app/Helpers/Formatter.php",
            "app/Helpers/menu.php",
            "app/Helpers/global.php",
            "app/Helpers/SequenceUtility.php",
            "app/Helpers/encryptionHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "repositories": []
}
