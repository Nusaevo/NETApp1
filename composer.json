{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "version": "1.0.0",
    "require": {
        "php": "^8.2",
        "anlutro/l4-settings": "^1.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.35",
        "laravel/sanctum": "^4.0.3",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "maatwebsite/excel": "^3.1",
        "psr/simple-cache": "^3.0",
        "rappasoft/laravel-livewire-tables": "^3.3.2",
        "ratchet/pawl": "^0.4.1",
        "resend/resend-laravel": "^0.22.0",
        "spatie/laravel-activitylog": "^4.8",
        "spatie/laravel-permission": "^6.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "barryvdh/laravel-ide-helper": "^3.2",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.37",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "nunomaduro/larastan": "^3.0",
        "phpunit/phpunit": "^11.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "files": [
            "app/Helpers/BootstrapHelpers.php",
            "app/Helpers/Formatter.php",
            "app/Helpers/menu.php",
            "app/Helpers/global.php",
            "app/Helpers/SequenceUtility.php",
            "app/Helpers/encryptionHelper.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
